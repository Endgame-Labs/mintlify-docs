  "navigation": {
    "pages": [
    {
      "group": "Getting Started",
      "pages": [
        "index",
        "core-concepts"
      ]
    },
    {
      "group": "Using Endgame",
      "pages": [
        "navigating-endgame",
        "endgame-chat-101",
        "endgame-chat-201"
      ]
    },
    {
      "group": "Features",
      "pages": [
        "features/templates",
        "features/publishing-artifacts"
      ]
    },
    {
      "group": "Integrations",
      "pages": [
        "integrations/getting-started",
        "integrations/salesforce",
        "integrations/gong",
        "integrations/slack",
        "integrations/zoom",
        "integrations/google-logins"
      ]
    },
    {
      "group": "Trust & Support",
      "pages": [
        "trust/security",
        "trust/privacy",
        "status",
        "contact"
      ]
    },
    {
      "group": "Resources",
      "pages": [
        "changelog",
        "changelog-consolidated",
        "platform",
        "support",
        "homepage"
      ]
    }
    ]
